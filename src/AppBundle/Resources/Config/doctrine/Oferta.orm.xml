<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Oferta" table="oferta">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreEmp" type="string" column="Nombre_Emp" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="Email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="Telefono" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="integer" column="Tipo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="Descripcion" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enlaces" type="string" column="Enlaces" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="validar" type="integer" column="Validar" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
