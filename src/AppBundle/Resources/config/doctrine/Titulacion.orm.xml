<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Titulacion" table="titulacion">
    <indexes>
      <index name="FK_titulacion_alumno" columns="Alumno"/>
      <index name="FK_titulacion_grado" columns="Grado"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="grado" target-entity="Grado" fetch="LAZY">
      <join-columns>
        <join-column name="Grado" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="alumno" target-entity="Usuario" fetch="LAZY">
      <join-columns>
        <join-column name="Alumno" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
