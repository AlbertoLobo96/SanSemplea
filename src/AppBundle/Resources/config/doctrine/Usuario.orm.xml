<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="usuario">
    <unique-constraints>
      <unique-constraint name="Email" columns="Email"/>
      <unique-constraint name="DNI" columns="DNI"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dni" type="string" column="DNI" length="9" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="Email" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="Nombre" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="Apellidos" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="curso" type="integer" column="Curso" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="telefono" type="string" column="Telefono" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="admin" type="integer" column="Admin" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="passwd" type="string" column="Passwd" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
